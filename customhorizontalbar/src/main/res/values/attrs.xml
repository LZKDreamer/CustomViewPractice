<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="bar_height" format="dimension"/>
    <attr name="bottom_color" format="color"/>
    <attr name="value_bar_color" format="color"/>
    <attr name="current_value" format="integer"/>
    <attr name="max_value" format="integer"/>
    <declare-styleable name="HorizontalBar">
        <attr name="bar_height" />
        <attr name="bottom_color"/>
        <attr name="value_bar_color"/>
        <attr name="current_value"/>
        <attr name="max_value"/>
        <attr name="value_text_color" format="color"/>
        <attr name="value_text_size" format="dimension"/>
        <attr name="value_text_visible" format="enum">
            <enum name="visible" value="0"/>
            <enum name="gone" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TagHorizontalBar">
        <attr name="bar_height" />
        <attr name="bottom_color"/>
        <attr name="value_bar_color"/>
        <attr name="current_value"/>
        <attr name="max_value"/>
        <attr name="tag_text_color" format="color"/>
        <attr name="tag_text_size" format="dimension"/>
        <attr name="tag_text" format="string"/>
        <attr name="tag_percent_text_size" format="dimension"/>
    </declare-styleable>
</resources>